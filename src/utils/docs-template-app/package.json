{
  "name": "@diez/docs-template-app",
  "version": "10.3.1",
  "private": true,
  "description": "Vue app used as a template for docs",
  "author": "The Haiku Team <contact@haiku.ai>",
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "fix": "vue-cli-service lint",
    "health": "yarn test && yarn lint",
    "test": "yarn test:e2e && yarn test:unit",
    "test:e2e": "ts-node ./script/prepare-tests.ts && vue-cli-service test:e2e --config nightwatch.config.js",
    "test:unit": "vue-cli-service test:unit",
    "pack": "npm pack"
  },
  "files": [
    "bin",
    "public/favicon.ico",
    "public/index.html",
    "dist",
    ".browserslistrc",
    "postcss.config.js",
    "README.md",
    "tsconfig.json",
    "tslint.json",
    "vue.config.js"
  ],
  "dependencies": {
    "http-server": "^0.10.0"
  },
  "devDependencies": {
    "@diez/docs-design-language": "^10.3.1",
    "@diez/engine": "^10.3.1",
    "@types/app-module-path": "^2.2.0",
    "@types/jest": "24.0.11",
    "@types/lunr": "^2.3.2",
    "@types/nightwatch": "^1.1.1",
    "@vue/cli-plugin-e2e-nightwatch": "^v4.0.0-rc.7",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "app-module-path": "^2.2.0",
    "chromedriver": "^79.0.0",
    "diez-webpack-plugin": "^10.3.1",
    "fs-extra": "^7.0.1",
    "geckodriver": "^1.18.0",
    "istanbul-reporter-cobertura-haiku": "^1.0.4",
    "lottie-web": "^5.5.7",
    "lunr": "^2.3.6",
    "nightwatch": "^1.2.4",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.17.1",
    "sass-loader": "^7.1.0",
    "split-grid": "^1.0.9",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "bin": {
    "docs-app": "./bin/docs-app.js"
  }
}
